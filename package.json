{
  "name": "webpack-multi",
  "version": "1.0.0",
  "description": "bootstrap of multi pages",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "env-cmd -e development webpack-dev-server --hot --watch --colors",
    "build": "env-cmd -e production webpack -p --display-reasons",
    "debug": "npm run start --inspect-brk test --runInBand --no-cache",
    "serve": "http-server ./build -p 8888 -o -i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:dev": "env-cmd -e lint webpack-dev-server --open",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx} --format table",
    "eslint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --format table --fix",
    "stylelint": "stylelint src/**/*.{css,less,sass,scss} --cache",
    "stylelint:fix": "stylelint src/**/*.{css,less,sass,scss} --cache --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,sass,scss,less,json,md,mdx}",
    "mdlint": "lint-md **/*.{md,markdown}",
    "mdlint:fix": "lint-md **/*.{md,markdown} --fix",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "jquery": "^1.12.4",
    "lodash-es": "^4.17.15",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ice/spec": "^1.0.1",
    "@ozo/mixins-sass": "^1.1.11",
    "autoprefixer": "^10.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^6.3.2",
    "core-js": "^3.8.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "ejs-compiled-loader": "^3.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "file-loader": "^6.2.0",
